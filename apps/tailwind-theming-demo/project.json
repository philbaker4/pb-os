{
  "name": "tailwind-theming-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tailwind-theming-demo",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/tailwind-theming-demo"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/tailwind-theming-demo"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "tailwind-theming-demo:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "tailwind-theming-demo:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "tailwind-theming-demo:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "tailwind-theming-demo:build:production"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tailwind-theming-demo/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
